// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model ram{
  id Int @id @default(autoincrement())
  quantite Int
}

model processeur{
  id Int @id @default(autoincrement())
  fournisseurprocid Int
  coeur Int
  generation Int
  frequence Int
  reference String
}

model fournisseurproc{
  id Int @id @default(autoincrement())
  nom String
}

