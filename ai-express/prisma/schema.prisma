// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model userAdmin {
  id    Int     @id @default(autoincrement())
  email String  @unique
  password String
  @@map(name: "admin")

}

model article {
  id    Int     @id @default(autoincrement())
  title String
  date DateTime
  description String
  contenu String
  @@map(name: "article")
}